// It is recommended that you DO NOT edit this file. Instead, make a copy that contains the settings you wish to change, and simply specify your copy on the command line:
// For example:
// python prs.py -s settings/my_settings.json
{
    "prompt" : "A druid in his shop, selling potions and trinkets, fantasy painting by raphael lacoste and craig mullins",
    "batch_name" : "Default", // Directory to save to inside "out"
    "width" : 512, // resolution
    "height" : 512, // resolution
    "steps" : 50, // Number of steps to do
    "seed" : "random", // Can be set to a specific number (no quotes)
    // Init image stuff
    "init_image": null, // the file to use for a starting point
    "init_strength": 0.5, // how strongly the init should be applied.
    // Generally, you can ignore most of the rest of this
    "n_batches" : 1, // Number of images to make in this render
    "skip_grid" : false, // Don't use the tile method when rendering multiple samples
    "skip_save" : false, // If for some weird reason you don't want to save your output
    "plms" : false, // whether or not do use plms diffusion instead of ddim
    "eta" : 0.0, // "corresponds to deterministic sampling"
    "n_iter" : 1, // number of samples per step?
    "n_samples" : 1, // how many samples to create in a given n_batch
    "n_rows" : 2, // width of the grid collage
    "scale" : 5.0, // clip guidance scale factor?
    "from_file": null // load prompts from a file (not supported yet)
}
